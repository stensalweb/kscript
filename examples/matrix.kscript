# matrix.kscript

A = [[1, 2], [3, 4]]
B = [[1, 0], [1, 1]]

matmul(a, b) := {
    if a[0].len != b.len {
        # error
    }
    c = [[0] * b[0].len] * a.len

    for i in 0:a.len {
        for j in 0:b[0].len {
            for k in 0:a[0].len {
                c[i][j] += a[i][k] * b[k][j]
            }
        }
    }
    return c
}

print (matmul(A, B))


