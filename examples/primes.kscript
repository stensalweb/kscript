
# primes.kscript - calculate the first `n` primes

func primes(n) {
  sieve = [true] * n
  nums = [2]

  i = 3
  while i * i < n {
    if sieve[i] {
      nums = nums + [i]
      j = i ^ 2
      while j < n {
        sieve[j] = false
        j = j + i
      }
    }
    i = i + 2
  }

  while i < n {
    if sieve[i] {
      nums = nums + [i]
    }
    i = i + 2
  }

  ret nums
}

x = primes(10) + (primes(20))

print (x)

